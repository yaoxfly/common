{
  "name": "@yaoxfly/verify-commit",
  "version": "1.0.1",
  "description": "Say goodbye to cumbersome configurations, an npx command automatically downloads husky, lint-staged, commitlint, commitizen, cz-customizable, etc., and completes the relevant configurations.",
  "scripts": {
    "dev": "tsc -w",
    "build:tsc": "tsc",
    "lint": "eslint . --ext .vue,.js,.ts,.jsx,.tsx --fix",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "bin": {
    "yx-verify-commit": "bin/src/index.js"
  },
  "files": [
    "bin",
    "husky",
    "commitlint"
  ],
  "keywords": [
    "husky",
    "lint-staged",
    "commitlint",
    "commitizen",
    "cz-customizable",
    "commitlint-config-cz",
    "git hooks"
  ],
  "type": "module",
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "author": "yaoxfly",
  "homepage": "https://github.com/yaoxfly/common/blob/main/packages/verify-commit/README.md",
  "license": "MIT",
  "engines": {
    "node": ">=16"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/yaoxfly/common.git",
    "directory": "packages/verify-commit"
  },
  "bugs": {
    "url": "https://github.com/yaoxfly/common/issues"
  },
  "dependencies": {
    "@yaoxfly/node-utils": "workspace:^",
    "commander": "^10.0.1"
  },
  "devDependencies": {
    "@yaoxfly/eslint-config": "workspace:^"
  }
}
